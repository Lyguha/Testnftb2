local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local auto = 0
local autoFarmEnabled = false
local currentMoney = 0

-- –ü–æ–ª—É—á–∞–µ–º —Å—Å—ã–ª–∫–∏ –Ω–∞ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–µ –æ–±—ä–µ–∫—Ç—ã –∏–≥—Ä—ã
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö –∫–µ–π—Å–æ–≤ —Å —Ü–µ–Ω–∞–º–∏
local casesDatabase = {
    -- –ë–µ—Å–ø–ª–∞—Ç–Ω—ã–µ –∫–µ–π—Å—ã
    ["Daily"] = 0,
    ["Trash"] = 0,
    
    -- –û—Å–Ω–æ–≤–Ω—ã–µ –∫–µ–π—Å—ã
    ["Beggar"] = 220,
    ["Plodder"] = 444,
    ["Office Clerk"] = 689,
    ["Manager"] = 835,
    ["Director"] = 926,
    ["Oligarch"] = 1364,
    
    -- –°–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ –∫–µ–π—Å—ã
    ["Frozen Heart"] = 4500,
    ["Bubble Gum"] = 12000,
    ["Cats"] = 40000,
    ["Glitch"] = 135000,
    ["Dream"] = 250000,
    ["Bloody Night"] = 5000000,
    
    -- NFT –∫–µ–π—Å—ã
    ["Gold"] = 1999,
    ["Dark"] = 4999,
    ["Palm"] = 13999,
    ["Buri"] = 37999,
    ["Luxury"] = 119999,
    
    -- –ê–≤—Ç–æ–º–æ–±–∏–ª–∏
    ["M5 F90"] = 55555,
    ["G63"] = 177777,
    ["Porsche 911"] = 599999,
    ["URUS"] = 1666666
}

-- –ö—ç—à –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞ –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏ –∫–µ–π—Å–æ–≤
local caseProfitabilityCache = {}
local caseHistory = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–µ–∫—É—â–µ–≥–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–∞ –¥–µ–Ω–µ–≥ –∏–∑ –∏–≥—Ä—ã
local function getMoney()
    local success, result = pcall(function()
        local starsValue = LocalPlayer:WaitForChild("_StarsValue")
        return starsValue.Value
    end)
    
    if success then
        currentMoney = result
        return result
    else
        warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–æ–ª—É—á–µ–Ω–∏–∏ –¥–µ–Ω–µ–≥: " .. tostring(result))
        return currentMoney
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞ —Å–æ–¥–µ—Ä–∂–∏–º–æ–≥–æ –∫–µ–π—Å–æ–≤
local function monitorCaseContents(caseName)
    if not caseHistory[caseName] then
        caseHistory[caseName] = {
            totalOpened = 0,
            totalCost = 0,
            totalRevenue = 0,
            items = {}
        }
    end
    return caseHistory[caseName]
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–ø–∏—Å–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –æ—Ç–∫—Ä—ã—Ç–∏—è –∫–µ–π—Å–∞
local function recordCaseResult(caseName, cost, itemsValue)
    local caseData = monitorCaseContents(caseName)
    caseData.totalOpened = caseData.totalOpened + 1
    caseData.totalCost = caseData.totalCost + cost
    caseData.totalRevenue = caseData.totalRevenue + itemsValue
    
    -- –†–∞—Å—Å—á–∏—Ç—ã–≤–∞–µ–º —Ç–µ–∫—É—â—É—é –æ–∫—É–ø–∞–µ–º–æ—Å—Ç—å
    if caseData.totalOpened > 0 then
        local avgReturn = caseData.totalRevenue / caseData.totalCost
        local profitMargin = ((caseData.totalRevenue - caseData.totalCost) / caseData.totalCost) * 100
        caseProfitabilityCache[caseName] = {
            avgReturn = avgReturn,
            profitMargin = profitMargin,
            confidence = math.min(1, caseData.totalOpened / 10) -- –£–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç—å –æ—Å–Ω–æ–≤–∞–Ω–∞ –Ω–∞ –∫–æ–ª–∏—á–µ—Å—Ç–≤–µ –æ—Ç–∫—Ä—ã—Ç–∏–π
        }
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–Ω–∞–ª–∏–∑–∞ –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –∫–µ–π—Å–æ–≤ –≤ –∏–≥—Ä–µ
local function analyzeAvailableCases()
    local availableCases = {}
    
    -- –ü—ã—Ç–∞–µ–º—Å—è –Ω–∞–π—Ç–∏ –¥–æ—Å—Ç—É–ø–Ω—ã–µ –∫–µ–π—Å—ã —á–µ—Ä–µ–∑ ReplicatedStorage
    local success, result = pcall(function()
        -- –ò—â–µ–º –≤–æ–∑–º–æ–∂–Ω—ã–µ –º–µ—Å—Ç–∞ –≥–¥–µ –º–æ–≥—É—Ç —Ö—Ä–∞–Ω–∏—Ç—å—Å—è –∫–µ–π—Å—ã
        local events = ReplicatedStorage:FindFirstChild("Events")
        if events then
            local openCaseEvent = events:FindFirstChild("OpenCase")
            if openCaseEvent then
                -- –ï—Å–ª–∏ –µ—Å—Ç—å —Å–æ–±—ã—Ç–∏–µ –æ—Ç–∫—Ä—ã—Ç–∏—è –∫–µ–π—Å–∞, –ø—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ–º —á—Ç–æ –≤—Å–µ –∫–µ–π—Å—ã –∏–∑ –±–∞–∑—ã –¥–æ—Å—Ç—É–ø–Ω—ã
                for caseName, price in pairs(casesDatabase) do
                    if currentMoney >= price then
                        table.insert(availableCases, {
                            name = caseName,
                            price = price,
                            affordable = true
                        })
                    end
                end
            end
        end
    end)
    
    if #availableCases == 0 then
        -- –ï—Å–ª–∏ –Ω–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ –∫–µ–π—Å—ã, –∏—Å–ø–æ–ª—å–∑—É–µ–º –±–∞–∑–æ–≤—ã–µ
        for caseName, price in pairs(casesDatabase) do
            if currentMoney >= price then
                table.insert(availableCases, {
                    name = caseName,
                    price = price,
                    affordable = true
                })
            end
        end
    end
    
    return availableCases
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ä–∞—Å—á–µ—Ç–∞ –æ–ø—Ç–∏–º–∞–ª—å–Ω–æ–≥–æ –∫–µ–π—Å–∞
local function calculateOptimalCase(availableCases)
    local bestCase = nil
    local bestScore = -math.huge
    
    for _, case in ipairs(availableCases) do
        local caseName = case.name
        local price = case.price
        
        -- –ë–∞–∑–æ–≤—ã–π –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç (—á–µ–º –¥–µ—à–µ–≤–ª–µ –∫–µ–π—Å –æ—Ç–Ω–æ—Å–∏—Ç–µ–ª—å–Ω–æ –±–∞–ª–∞–Ω—Å–∞, —Ç–µ–º –ª—É—á—à–µ)
        local affordability = (currentMoney - price) / currentMoney
        
        -- –£—á–∏—Ç—ã–≤–∞–µ–º –∏—Å—Ç–æ—Ä–∏—é –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏ –µ—Å–ª–∏ –µ—Å—Ç—å
        local profitabilityScore = 1.0 -- –Ω–µ–π—Ç—Ä–∞–ª—å–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
        local confidence = 0.1 -- –Ω–∏–∑–∫–∞—è —É–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç—å –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
        
        if caseProfitabilityCache[caseName] then
            local cache = caseProfitabilityCache[caseName]
            profitabilityScore = cache.avgReturn
            confidence = cache.confidence
        end
        
        -- –®—Ç—Ä–∞—Ñ—É–µ–º —Å–ª–∏—à–∫–æ–º –¥–æ—Ä–æ–≥–∏–µ –∫–µ–π—Å—ã –Ω–∞ –Ω–∞—á–∞–ª—å–Ω–æ–º —ç—Ç–∞–ø–µ
        local pricePenalty = 0
        if price > currentMoney * 0.1 then -- –ï—Å–ª–∏ –∫–µ–π—Å –¥–æ—Ä–æ–∂–µ 10% –±–∞–ª–∞–Ω—Å–∞
            pricePenalty = (price / (currentMoney * 0.1)) - 1
        end
        
        -- –ü–æ–æ—â—Ä—è–µ–º –¥–∏–≤–µ—Ä—Å–∏—Ñ–∏–∫–∞—Ü–∏—é (–ø—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –∫–µ–π—Å—ã)
        local diversityBonus = 0
        if caseHistory[caseName] and caseHistory[caseName].totalOpened < 3 then
            diversityBonus = 0.5 * (3 - caseHistory[caseName].totalOpened) / 3
        end
        
        -- –ò—Ç–æ–≥–æ–≤—ã–π score
        local score = (profitabilityScore * confidence) + (affordability * 0.3) - (pricePenalty * 0.2) + diversityBonus
        
        -- –ë–µ—Å–ø–ª–∞—Ç–Ω—ã–µ –∫–µ–π—Å—ã –≤—Å–µ–≥–¥–∞ –∏–º–µ—é—Ç –≤—ã—Å–æ–∫–∏–π –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç
        if price == 0 then
            score = score + 2
        end
        
        -- –£—á–∏—Ç—ã–≤–∞–µ–º –ø—Ä–æ–≥—Ä–µ—Å—Å–∏—é - –ø–æ—Å—Ç–µ–ø–µ–Ω–Ω–æ –ø–µ—Ä–µ—Ö–æ–¥–∏–º –∫ –±–æ–ª–µ–µ –¥–æ—Ä–æ–≥–∏–º –∫–µ–π—Å–∞–º
        local progressionBonus = 0
        if price > 0 and price <= currentMoney * 0.05 then -- –ö–µ–π—Å—ã –¥–æ 5% –æ—Ç –±–∞–ª–∞–Ω—Å–∞
            progressionBonus = 0.3
        end
        
        score = score + progressionBonus
        
        if score > bestScore then
            bestScore = score
            bestCase = case
        end
    end
    
    return bestCase
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ü–µ–Ω–∫–∏ —Å—Ç–æ–∏–º–æ—Å—Ç–∏ –≤—ã–ø–∞–≤—à–∏—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
local function estimateItemsValue()
    -- –≠—Ç–∞ —Ñ—É–Ω–∫—Ü–∏—è –¥–æ–ª–∂–Ω–∞ –∞–Ω–∞–ª–∏–∑–∏—Ä–æ–≤–∞—Ç—å –≤—ã–ø–∞–≤—à–∏–µ –ø—Ä–µ–¥–º–µ—Ç—ã –∏ –æ—Ü–µ–Ω–∏–≤–∞—Ç—å –∏—Ö —Å—Ç–æ–∏–º–æ—Å—Ç—å
    -- –í —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –Ω—É–∂–Ω–æ –ø–∞—Ä—Å–∏—Ç—å –∏–Ω–≤–µ–Ω—Ç–∞—Ä—å –∏–ª–∏ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –¥–∞–Ω–Ω—ã–µ –∏–∑ –∏–≥—Ä—ã
    -- –î–ª—è —É–ø—Ä–æ—â–µ–Ω–∏—è –±—É–¥–µ–º –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —ç–≤—Ä–∏—Å—Ç–∏—á–µ—Å–∫—É—é –æ—Ü–µ–Ω–∫—É
    
    local baseValue = 0
    
    -- –≠–≤—Ä–∏—Å—Ç–∏–∫–∞: —á–µ–º –¥–æ—Ä–æ–∂–µ –∫–µ–π—Å, —Ç–µ–º —Ü–µ–Ω–Ω–µ–µ –ø—Ä–µ–¥–º–µ—Ç—ã (–≤ —Å—Ä–µ–¥–Ω–µ–º)
    -- –í —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –Ω—É–∂–Ω–æ –∞–Ω–∞–ª–∏–∑–∏—Ä–æ–≤–∞—Ç—å —Ñ–∞–∫—Ç–∏—á–µ—Å–∫–∏–π –≤—ã–ø–∞–¥
    if currentMoney < 1000 then
        baseValue = math.random(50, 200)
    elseif currentMoney < 10000 then
        baseValue = math.random(200, 1000)
    elseif currentMoney < 100000 then
        baseValue = math.random(1000, 5000)
    else
        baseValue = math.random(5000, 25000)
    end
    
    -- –î–æ–±–∞–≤–ª—è–µ–º —Å–ª—É—á–∞–π–Ω–æ—Å—Ç—å –¥–ª—è –∏–º–∏—Ç–∞—Ü–∏–∏ —Ä–∞–∑–Ω—ã—Ö –≤—ã–ø–∞–¥–µ–Ω–∏–π
    local multiplier = math.random(70, 150) / 100
    return baseValue * multiplier
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–¥–∞–∂–∏ –≤—Å–µ—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
local function sellAllItems()
    local success, errorMsg = pcall(function()
        local args = {"Sell", "ALL"}
        game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Inventory"):FireServer(unpack(args))
    end)
    
    if success then
        print("–£—Å–ø–µ—à–Ω–æ –ø—Ä–æ–¥–∞–Ω—ã –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã")
        return true
    else
        warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø—Ä–æ–¥–∞–∂–µ –ø—Ä–µ–¥–º–µ—Ç–æ–≤: " .. tostring(errorMsg))
        return false
    end
end

-- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–≥–æ —Ñ–∞—Ä–º–∞
local function intelligentAutoFarm()
    while autoFarmEnabled do
        -- –ü–æ–ª—É—á–∞–µ–º –∞–∫—Ç—É–∞–ª—å–Ω—ã–π –±–∞–ª–∞–Ω—Å
        currentMoney = getMoney()
        
        -- –ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º –¥–æ—Å—Ç—É–ø–Ω—ã–µ –∫–µ–π—Å—ã
        local availableCases = analyzeAvailableCases()
        
        if #availableCases == 0 then
            warn("–ù–µ—Ç –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –∫–µ–π—Å–æ–≤ –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è")
            wait(2)
            continue
        end
        
        -- –í—ã—á–∏—Å–ª—è–µ–º –æ–ø—Ç–∏–º–∞–ª—å–Ω—ã–π –∫–µ–π—Å
        local optimalCase = calculateOptimalCase(availableCases)
        
        if not optimalCase then
            warn("–ù–µ —É–¥–∞–ª–æ—Å—å –æ–ø—Ä–µ–¥–µ–ª–∏—Ç—å –æ–ø—Ç–∏–º–∞–ª—å–Ω—ã–π –∫–µ–π—Å")
            wait(2)
            continue
        end
        
        -- –û—Ç–∫—Ä—ã–≤–∞–µ–º –≤—ã–±—Ä–∞–Ω–Ω—ã–π –∫–µ–π—Å
        local openSuccess, openError = pcall(function()
            local args = {optimalCase.name, 1} -- –û—Ç–∫—Ä—ã–≤–∞–µ–º –ø–æ 1 –∫–µ–π—Å—É –¥–ª—è –ª—É—á—à–µ–≥–æ –∫–æ–Ω—Ç—Ä–æ–ª—è
            game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCase"):InvokeServer(unpack(args))
        end)
        
        if openSuccess then
            print("–£—Å–ø–µ—à–Ω–æ –æ—Ç–∫—Ä—ã—Ç –∫–µ–π—Å: " .. optimalCase.name .. " (–¶–µ–Ω–∞: " .. optimalCase.price .. ")")
            
            -- –ñ–¥–µ–º –æ–±—Ä–∞–±–æ—Ç–∫–∏ –æ—Ç–∫—Ä—ã—Ç–∏—è
            wait(0.5)
            
            -- –û—Ü–µ–Ω–∏–≤–∞–µ–º —Å—Ç–æ–∏–º–æ—Å—Ç—å –≤—ã–ø–∞–≤—à–∏—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ (–≤ —Ä–µ–∞–ª—å–Ω–æ–π —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –Ω—É–∂–Ω–æ –ø–∞—Ä—Å–∏—Ç—å –∏–Ω–≤–µ–Ω—Ç–∞—Ä—å)
            local estimatedValue = estimateItemsValue()
            
            -- –ó–∞–ø–∏—Å—ã–≤–∞–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç –¥–ª—è –±—É–¥—É—â–µ–≥–æ –∞–Ω–∞–ª–∏–∑–∞
            recordCaseResult(optimalCase.name, optimalCase.price, estimatedValue)
            
            -- –ü—Ä–æ–¥–∞–µ–º –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã
            local sellSuccess = sellAllItems()
            
            if sellSuccess then
                -- –û–±–Ω–æ–≤–ª—è–µ–º –±–∞–ª–∞–Ω—Å –ø–æ—Å–ª–µ –ø—Ä–æ–¥–∞–∂–∏
                wait(0.5)
                currentMoney = getMoney()
                
                -- –õ–æ–≥–∏—Ä—É–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
                local caseStats = caseHistory[optimalCase.name]
                if caseStats and caseStats.totalOpened > 0 then
                    local avgReturn = caseStats.totalRevenue / caseStats.totalCost
                    local profitMargin = ((caseStats.totalRevenue - caseStats.totalCost) / caseStats.totalCost) * 100
                    print(string.format("–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –∫–µ–π—Å–∞ %s: –û—Ç–∫—Ä—ã—Ç–æ %d, –û–∫—É–ø–∞–µ–º–æ—Å—Ç—å: %.2fx (%.1f%%)", 
                          optimalCase.name, caseStats.totalOpened, avgReturn, profitMargin))
                end
            end
            
        else
            warn("–û—à–∏–±–∫–∞ –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏ –∫–µ–π—Å–∞ " .. optimalCase.name .. ": " .. tostring(openError))
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∏—Ç–µ—Ä–∞—Ü–∏–π
        auto += 1
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        updateFarmStats()
        
        -- –î–∏–Ω–∞–º–∏—á–µ—Å–∫–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Å—Ç–æ–∏–º–æ—Å—Ç–∏ –∫–µ–π—Å–∞
        local delay = math.max(0.5, optimalCase.price / 10000) -- –ß–µ–º –¥–æ—Ä–æ–∂–µ –∫–µ–π—Å, —Ç–µ–º –¥–æ–ª—å—à–µ –∂–¥–µ–º
        wait(delay)
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ —Ñ–∞—Ä–º–∞
local function updateFarmStats()
    if FarmStatsLabel then
        local totalOpened = 0
        local totalProfit = 0
        
        for caseName, data in pairs(caseHistory) do
            totalOpened = totalOpened + data.totalOpened
            totalProfit = totalProfit + (data.totalRevenue - data.totalCost)
        end
        
        FarmStatsLabel:Set(string.format("–û—Ç–∫—Ä—ã—Ç–æ: %d | –ü—Ä–∏–±—ã–ª—å: %d", totalOpened, totalProfit))
    end
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ–∫–Ω–∞ Rayfield (–≤–∞—à —Å—É—â–µ—Å—Ç–≤—É—é—â–∏–π –∫–æ–¥)
local Window = Rayfield:CreateWindow({
   Name = "–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã–π –∞–≤—Ç–æ—Ñ–∞—Ä–º –∫–µ–π—Å–æ–≤",
   Icon = 0,
   LoadingTitle = "–£–º–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ —Ñ–∞—Ä–º–∞",
   LoadingSubtitle = "–ê–Ω–∞–ª–∏–∑ –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏ –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏",
   ShowText = "–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã–π –∞–≤—Ç–æ—Ñ–∞—Ä–º",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {Enabled = false},
   Discord = {Enabled = false},
   KeySystem = false
})

local Tab8 = Window:CreateTab("–£–º–Ω—ã–π –§–∞—Ä–º", "settings")
local Tab1 = Window:CreateTab("Credits", "creative-commons")

-- –≠–ª–µ–º–µ–Ω—Ç—ã –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
local Label = Tab8:CreateLabel("–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ —Ñ–∞—Ä–º–∞ –∫–µ–π—Å–æ–≤")
local Label = Tab8:CreateLabel("–°–∏—Å—Ç–µ–º–∞ –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –æ–∫—É–ø–∞–µ–º–æ—Å—Ç—å –∫–µ–π—Å–æ–≤ –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏")
local Label = Tab8:CreateLabel("–∏ –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –≤—ã–±–∏—Ä–∞–µ—Ç —Å–∞–º—ã–π –≤—ã–≥–æ–¥–Ω—ã–π –≤–∞—Ä–∏–∞–Ω—Ç")

local CurrentMoneyLabel = Tab8:CreateLabel("–¢–µ–∫—É—â–∏–µ –¥–µ–Ω—å–≥–∏: 0")
local OptimalCaseLabel = Tab8:CreateLabel("–†–µ–∫–æ–º–µ–Ω–¥—É–µ–º—ã–π –∫–µ–π—Å: –ù–µ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω")
local FarmStatsLabel = Tab8:CreateLabel("–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞: –ù–µ –∞–∫—Ç–∏–≤–µ–Ω")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–µ–Ω–µ–≥ –∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–π
local function updateMoneyDisplay()
    currentMoney = getMoney()
    CurrentMoneyLabel:Set("–¢–µ–∫—É—â–∏–µ –¥–µ–Ω—å–≥–∏: " .. tostring(currentMoney))
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—é
    if autoFarmEnabled then
        local availableCases = analyzeAvailableCases()
        local optimalCase = calculateOptimalCase(availableCases)
        if optimalCase then
            local confidence = caseProfitabilityCache[optimalCase.name] and 
                             caseProfitabilityCache[optimalCase.name].confidence or 0.1
            OptimalCaseLabel:Set(string.format("–†–µ–∫–æ–º–µ–Ω–¥—É–µ–º—ã–π –∫–µ–π—Å: %s (—É–≤–µ—Ä–µ–Ω–Ω–æ—Å—Ç—å: %.1f%%)", 
                optimalCase.name, confidence * 100))
        end
    end
end

-- –ö–Ω–æ–ø–∫–∞ –∑–∞–ø—É—Å–∫–∞ —É–º–Ω–æ–≥–æ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞
local ToggleButton = Tab8:CreateToggle({
    Name = "–ó–∞–ø—É—Å–∫ —É–º–Ω–æ–≥–æ –∞–≤—Ç–æ—Ñ–∞—Ä–º–∞",
    CurrentValue = false,
    Flag = "SmartAutoFarmToggle",
    Callback = function(Value)
        autoFarmEnabled = Value
        if Value then
            Rayfield:Notify({
                Title = "–£–º–Ω—ã–π –∞–≤—Ç–æ—Ñ–∞—Ä–º –∑–∞–ø—É—â–µ–Ω",
                Content = "–°–∏—Å—Ç–µ–º–∞ –∞–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –æ–∫—É–ø–∞–µ–º–æ—Å—Ç—å –∏ –≤—ã–±–∏—Ä–∞–µ—Ç –æ–ø—Ç–∏–º–∞–ª—å–Ω—ã–µ –∫–µ–π—Å—ã",
                Duration = 3,
                Image = "‚úì",
            })
            -- –ó–∞–ø—É—Å–∫–∞–µ–º —É–º–Ω—ã–π –∞–≤—Ç–æ—Ñ–∞—Ä–º
            coroutine.wrap(intelligentAutoFarm)()
        else
            Rayfield:Notify({
                Title = "–ê–≤—Ç–æ—Ñ–∞—Ä–º –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω",
                Content = "–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã–π —Ñ–∞—Ä–º –æ—Ç–∫–ª—é—á–µ–Ω",
                Duration = 3,
                Image = "‚úó",
            })
        end
    end,
})

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è —Å–±—Ä–æ—Å–∞ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
local Button = Tab8:CreateButton({
    Name = "–°–±—Ä–æ—Å–∏—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É",
    Callback = function()
        caseProfitabilityCache = {}
        caseHistory = {}
        Rayfield:Notify({
            Title = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ —Å–±—Ä–æ—à–µ–Ω–∞",
            Content = "–ò—Å—Ç–æ—Ä–∏—è –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏ –æ—á–∏—â–µ–Ω–∞",
            Duration = 3,
            Image = "‚úì",
        })
    end,
})

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è –ø—Ä–∏–Ω—É–¥–∏—Ç–µ–ª—å–Ω–æ–π –ø—Ä–æ–¥–∞–∂–∏
local Button = Tab8:CreateButton({
    Name = "–ü—Ä–æ–¥–∞—Ç—å –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã",
    Callback = function()
        sellAllItems()
        updateMoneyDisplay()
    end,
})

-- –ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –ø–æ –∫–µ–π—Å–∞–º
local Button = Tab8:CreateButton({
    Name = "–ü–æ–∫–∞–∑–∞—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –∫–µ–π—Å–æ–≤",
    Callback = function()
        local statsText = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –æ–∫—É–ø–∞–µ–º–æ—Å—Ç–∏:\n"
        for caseName, data in pairs(caseHistory) do
            if data.totalOpened > 0 then
                local avgReturn = data.totalRevenue / data.totalCost
                local profitMargin = ((data.totalRevenue - data.totalCost) / data.totalCost) * 100
                statsText = statsText .. string.format("%s: %d –æ—Ç–∫—Ä., %.2fx –æ–∫—É–ø. (%.1f%%)\n", 
                    caseName, data.totalOpened, avgReturn, profitMargin)
            end
        end
        
        Rayfield:Notify({
            Title = "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –∫–µ–π—Å–æ–≤",
            Content = statsText,
            Duration = 8,
            Image = "üìä",
        })
    end,
})

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
coroutine.wrap(function()
    while true do
        if autoFarmEnabled then
            updateMoneyDisplay()
            updateFarmStats()
        end
        wait(2)
    end
end)()

-- –í–∞—à–∏ –æ—Å—Ç–∞–ª—å–Ω—ã–µ –≤–∫–ª–∞–¥–∫–∏...
local Label = Tab1:CreateLabel("–ö–æ–¥–µ—Ä: –ê–≤–µ–¥")
local Label = Tab1:CreateLabel("–¢–µ—Å—Ç–µ—Ä: –ê–≤–µ–¥, –î—ç–º—ç–Ω—Ç—ã–π")
local Label = Tab1:CreateLabel("–ì—É–∏: Sirius")
local Label = Tab1:CreateLabel("–ú–∞—Ä–∏—è: –ó–∞–π—Ü–µ–≤–∞")

Rayfield:Notify({
   Title = "–ò–Ω—Ç–µ–ª–ª–µ–∫—Ç—É–∞–ª—å–Ω—ã–π —Ñ–∞—Ä–º –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω",
   Content = "–°–∏—Å—Ç–µ–º–∞ –±—É–¥–µ—Ç –∞–Ω–∞–ª–∏–∑–∏—Ä–æ–≤–∞—Ç—å –æ–∫—É–ø–∞–µ–º–æ—Å—Ç—å –∫–µ–π—Å–æ–≤ –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏",
   Duration = 6.5,
   Image = "üß†",
})
